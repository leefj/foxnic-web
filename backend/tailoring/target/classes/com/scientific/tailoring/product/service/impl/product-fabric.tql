[page-list]
SELECT pf.*,p.name product_name,f.short_name fabric_name,vl.effect_file_id from prd_product_fabric pf 
join prd_product p on pf.product_id=p.id and p.deleted=0
join prd_fabric f on pf.fabric_id=f.id and f.deleted=0
join prd_virtual_library vl on pf.virtual_id=vl.id and vl.valid=1 and vl.deleted=0
where pf.product_id=? 


